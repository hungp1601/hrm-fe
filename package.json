{
  "name": "hrm-app",
  "version": "2.0.12",
  "description": "hrm application",
  "author": "hungkb16",
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "./nuxt.config.ts",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "assets",
    "components",
    "composables",
    "layouts",
    "modules",
    "pages",
    "public",
    "stores",
    "app.config.ts",
    "tailwind.config.ts",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "nuxt build ./app",
    "dev": "nuxt dev ./app --no-clear",
    "generate": "nuxt generate ./app",
    "preview": "nuxt preview ./app",
    "postinstall": "nuxt prepare ./app",
    "dev:starter": "nuxt dev ./examples/starter",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "release": "release-it",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/tailwindcss": "^6.8.1",
    "@pinia/nuxt": "^0.5.1",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/nuxt": "^10.5.0",
    "element-plus": "^2.4.3",
    "nuxt-headlessui": "^1.1.4",
    "nuxt-icon": "^0.6.1",
    "nuxt-logger": "^0.2.0",
    "nuxt-socket-io": "^3.0.13",
    "pinia": "^2.1.7",
    "sass": "^1.69.5",
    "stringify": "^5.2.0",
    "sweetalert2": "^11.10.1",
    "tailwindcss": "^3.3.5",
    "unplugin-vue-components": "^0.25.2",
    "vue-draggable-next": "^2.2.1",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vuejs3-logger": "^1.0.0"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.7",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/svg-sprite": "^1.0.2",
    "@nuxtjs/toast": "^3.3.1",
    "@types/lodash": "^4.14.202",
    "consola": "^3.2.3",
    "dayjs-nuxt": "^2.1.9",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "nuxt": "^3.8.0",
    "nuxt-bootstrap-icons": "^1.0.3",
    "nuxt-lodash": "^2.5.3",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "serve": "^14.2.1",
    "typescript": "^5.2.2",
    "vue-toast-notification": "^3.1.2",
    "vue-toastification": "^1.7.14",
    "vue-tsc": "1.8.22",
    "vuedraggable": "^4.1.0"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    }
  }
}
