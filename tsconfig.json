{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./app/.nuxt/tsconfig.json",
  "rootDir": "./",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "ignoreDeprecations": "5.0",
    "suppressImplicitAnyIndexErrors": true,
    "types": ["@pinia/nuxt", "@nuxtjs/tailwindcss"],
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "@/Utils/*": ["./utils/*"],
      "@/Utils": ["./utils"],
      "@Utils/*": ["./utils/*"],
      "@Utils": ["./utils"],
      "@App/*": ["./app/*"],
    }
  }
}
