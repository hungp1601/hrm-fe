{
  "root": true,
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-multiple-template-root": "off",
    "no-console": "off",
    "vue/require-default-prop": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "camelcase": 0
  }
}
